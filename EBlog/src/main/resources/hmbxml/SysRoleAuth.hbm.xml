<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.blog.po.SysRoleAuth" table="sys_roleauth" catalog="eblog" optimistic-lock="version">
		<id name="id" type="string">
			<column name="ID" length="60" />
		</id>
		<property name="createTime" type="timestamp" update="false" insert="false">
			<column name="CreateTime" length="0" />
		</property>
		<property name="creator" type="string" update="false">
			<column name="Creator" length="60" />
		</property>
		<property name="modifyTime" type="timestamp" update="false" insert="false">
			<column name="ModifyTime" length="0" />
		</property>
		<property name="modifier" type="string" insert="false">
			<column name="Modifier" length="60" />
		</property>
		<property name="roleId" type="string">
			<column name="RoleId" length="60" />
		</property>
		<property name="menuId" type="string">
			<column name="MenuId" length="60" />
		</property>
	</class>
</hibernate-mapping>
